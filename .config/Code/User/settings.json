{
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontSize": 10,
  "zenMode.hideLineNumbers": false,
  "telemetry.telemetryLevel": "off",
  "workbench.colorTheme": "Zenburn",
  "editor.cursorStyle": "block",
  "editor.renderLineHighlight": "gutter",
  "editor.roundedSelection": false,
  "workbench.preferredDarkColorTheme": "Zenburn",
  "extensions.ignoreRecommendations": true,
  "window.enableMenuBarMnemonics": false,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "terminal.integrated.fontFamily": "'FantasqueSansMono Nerd Font'",
  "terminal.integrated.fontSize": 10,
  "workbench.tree.indent": 18,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "material-icon-theme",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "editor.tabSize": 2,
  "editor.minimap.size": "fit",
  "editor.glyphMargin": false,
  "editor.renderFinalNewline": false,
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.scrollback": 10000,
  "gitProjectManager.baseProjectsFolders": [
    "/home/mark/Wst/GitLab",
    "/home/mark/SiofokiAllatvedo/GitHub",
    "/home/mark/GitHub",
    "/home/mark/GitLab"
  ],
  "git.confirmSync": false,
  "git.autofetch": true,
  "numberedBookmarks.navigateThroughAllFiles": "replace",
  "numberedBookmarks.saveBookmarksInProject": true,
  "explorer.confirmDelete": false,
  "daml.telemetry": "Disable",
  "window.menuBarVisibility": "toggle",
  "metals.superMethodLensesEnabled": false,
  "editor.fontLigatures": true,
  "editor.fontFamily": "FantasqueSansMono Nerd Font, Hasklug Nerd Font, VictorMono Nerd Font, Hack, Meslo LG M, Inconsolata, 'Source Code Pro for Powerline', 'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "coq.interpretToEndOfSentence": true,
  "files.exclude": {
    "**/.*.aux": true,
    "**/*.glob": true,
    "**/*.vo": true,
    "**/*.vok": true,
    "**/*.vos": true
  },
  "[haskell]": {
    "editor.defaultFormatter": "MaxGabriel.brittany"
  },
  "javascript.format.semicolons": "remove",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "notebook.lineNumbers": "on",
  "haskell.manageHLS": "PATH",
  "editor.guides.indentation": false,
  "editor.bracketPairColorization.enabled": false,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.chktex.run": "onType",
  "latex-workshop.latex.autoBuild.run": "onSave",
  "haskell.formattingProvider": "brittany",
  "emacs-mcx.strictEmacsMove": true,
  "githubPullRequests.pullBranch": "never",
  "triggerTaskOnSave.showStatusBarToggle": true,
  "triggerTaskOnSave.tasks": {
    "build": ["*.hs"]
  },
  "triggerTaskOnSave.on": false,
  "commentAnchors.workspace.excludeFiles": "**/{node_modules,.git,.idea,target,out,build,dist,dist-newstyle,vendor}/**/*"
}
